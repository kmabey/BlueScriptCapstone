@model IEnumerable<Blue_Script.Setting>
@for (var s = 0; s < Model.Count(); s++)
{
	<div class="event well" draggable="true">
		@Html.Action("EditSetting", "Home", new { id = Model.ElementAt(s).ID })
	</div>
}
<div class="event add">
	<a href="@Url.Action("CreateSetting", "Home")"><i class="icon-plus-sign-alt icon-4x">
	</i></a>
</div>
